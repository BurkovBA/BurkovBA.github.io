{
  "name": "burkov",
  "version": "2.0.0",
  "description": "Personal blog of Boris Burkov",
  "scripts": {
    "clean": "rm -rf dist/*",
    "build:prod": "node_modules/.bin/webpack -p --env.prod",
    "build:ssr": "node_modules/.bin/webpack -p --env.ssr",
    "build": "npm run build:prod && npm run build:ssr",
    "profile:build": "node_modules/.bin/webpack -p --env.prod --profile --json > webpack-stats.json",
    "serve": "node_modules/.bin/webpack-dev-server --progress --colors --history-api-fallback",
    "ssr": "node_modules/.bin/webpack -p --env.ssr && node dist/server.js",
    "start": "node dist/server.js",
    "profile:ssr": "node_modules/.bin/webpack -p --env.prod --config ssr-webpack.config.js --profile --json > webpack-stats.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "npm run clean && npm run build && git add dist && git add index.html && git commit -a -m 'deploy' && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BurkovBA/BurkovBA.github.io"
  },
  "keywords": [
    "blog",
    "world view",
    "engineering",
    "entrepreneurship",
    "economy",
    "history",
    "physics",
    "programming",
    "music",
    "mathematics"
  ],
  "author": "Boris A. Burkov",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/BurkovBA/BurkovBA.github.io/issues"
  },
  "homepage": "https://github.com/BurkovBA/BurkovBA.github.io#readme",
  "dependencies": {
    "animate.css": "^3.5.2",
    "bootstrap": "^3.3.7",
    "flag-icon-css": "^2.8.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "metismenu": "^2.7.4",
    "pixeden-stroke-7-icon": "^1.2.3",
    "react": "^16.1.1",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.1.1",
    "react-gist-burkov": "^1.1.3",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "jsdom": "^11.6.2",
    "navigator": "~1.0.1",
    "node-sass": "^4.5.3",
    "react-router-config": "^1.0.0-beta.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-node-externals": "^1.6.0"
  }
}
